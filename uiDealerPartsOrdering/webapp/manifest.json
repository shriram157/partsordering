				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Both",
					"json": true,
					"useBatch": true
				},
				"dataSource": "MM_PUR_PO_MAINT_V2_SRV",
