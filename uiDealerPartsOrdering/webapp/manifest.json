					"defaultCountMode": "Request",
					"json": true,
					"useBatch": true
				},
				"dataSource": "API_PURCHASEORDER_PROCESS_SRV",
				"preload": false
			},
			"API_PRODUCT_SRV": {
				"uri": "/sap/opu/odata/sap/API_PRODUCT_SRV/",
