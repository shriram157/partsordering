		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "tci.wave2.ui.parts.ordering.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.38.34",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "tci.wave2.ui.parts.ordering.i18n.i18n"
				}
			},
			"API_BUSINESS_PARTNER": {
				"uri": "/sap/opu/odata/sap/API_BUSINESS_PARTNER/",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request",
					"json": true,
					"useBatch": true
				},
				"dataSource": "API_BUSINESS_PARTNER",
				"preload": false
			},
			"API_PURCHASEORDER_PROCESS_SRV": {
				"uri": "/sap/opu/odata/sap/API_PURCHASEORDER_PROCESS_SRV/",
