					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request",
					"json": true,
					"useBatch": true
				},
				"dataSource": "API_PRODUCT_SRV",
				"preload": false
			},
			"MD_PRODUCT_FS_SRV": {
				"uri": "/sap/opu/odata/sap/MD_PRODUCT_FS_SRV/",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
