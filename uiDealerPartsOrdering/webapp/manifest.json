		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "tci.wave2.ui.parts.ordering.view",
				"controlAggregation": "pages",
				"controlId": "rootControl",
				"clearControlAggregation": false
			},
			"routes": [{
				"pattern": "",
				"name": "StartOrdering",
				"target": ["StartOrdering"]
			}, {
				"pattern": "CreateOrder",
				"name": "CreateOrder",
				"target": ["CreateOrder"]
			}, {
				"pattern": "FindOrder",
				"name": "FindOrder",
				"target": ["FindOrder"]
			}, {
				"pattern": "CheckOrderStatus",
				"name": "CheckOrderStatus",
				"target": ["CheckOrderStatus"]
			}],
			"targets": {
				"StartOrdering": {
					"viewName": "StartOrdering",
					"viewType": "XML",
					"clearControlAggregation": false,
					"viewLevel": 1,
					"transition": "slide"
				},
				"CreateOrder": {
					"viewName": "CreateOrder",
					"viewType": "XML",
					"clearControlAggregation": false,
					"viewLevel": 2,
					"transition": "slide"
				},
				"FindOrder": {
					"viewName": "FindOrder",
					"viewType": "XML",
					"clearControlAggregation": false,
					"viewLevel": 1,
					"transition": "slide"
				},
				"CheckOrderStatus": {
					"viewName": "CheckOrderStatus",
					"viewType": "XML",
					"clearControlAggregation": false,
					"viewLevel": 1,
					"transition": "slide"
				}
			}
		}
